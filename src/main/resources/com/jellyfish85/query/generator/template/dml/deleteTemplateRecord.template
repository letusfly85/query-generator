CONN ${executor}/&1

spool DELETE_log/${schemaName}.${tableName}.log
DELETE FROM ${schemaName}.${tableName} tpl
WHERE
    tpl.${columnName} IN
(
<% recordIds.eachWithIndex { recordId, index ->
    if (index == 0) { %>\
     '${recordId}'
<% } else {%>\
    ,'${recordId}'
<%
        }
    } %>\
)
;
COMMIT;
EXIT;